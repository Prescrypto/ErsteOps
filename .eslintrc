{
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:vue/recommended", "prettier"],
  "plugins": ["prettier"],
    "settings": {
      "import/resolver": "webpack"
    },
  "rules": {
    "no-param-reassign": 0
  }
}
